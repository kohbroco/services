<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <defaultDocument>
            <files>
                <clear />
                <add value="Default.htm" />
                <add value="Default.asp" />
                <add value="index.htm" />
                <add value="index.php" />
                <add value="index.html" />
                <add value="iisstart.htm" />
                <add value="default.aspx" />
            </files>
        </defaultDocument>
        <handlers>
            <remove name="PHP_via_FastCGI" />
            <remove name="PHP 7.0.11 non-thread-safe" />
            <remove name="PHP53_via_FastCGI" />
            <add name="PHP 7.1.0" path="*.php" verb="*" modules="FastCgiModule" scriptProcessor="C:\php\7.1.0\php-cgi.exe" resourceType="File" />
        </handlers>
    </system.webServer>
</configuration>
